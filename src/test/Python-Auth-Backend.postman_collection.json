				{
					"name": "/v1/auth/login",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "493d5c2e-9166-4fcb-a643-96424cb2d68d",
								"exec": [
									"var jsonData = pm.response.json();",
									"pm.test(\"jwt_token not empty\", function () {",
									"    pm.expect(jsonData.token).not.eql(undefined);",
									"});",
									"",
									"// set access token as environement variable",
									"if (jsonData.token !== undefined) {",
									"    postman.setEnvironmentVariable(\"jwt_token\", jsonData.token);",
									"} else {",
									"    postman.setEnvironmentVariable(\"access_token\", null);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
